// sort //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
function sort(io Scalar scalarArray[]){
  Scalar tmpS;
  for(Integer i=0; i<scalarArray.size(); i++){
    for(Integer j=i+1; j<scalarArray.size(); j++) {
      if (scalarArray[i] > scalarArray[j]) {
        tmpS =  scalarArray[i];
        scalarArray[i] = scalarArray[j];
        scalarArray[j] = tmpS;
      }
    }
  }
}
